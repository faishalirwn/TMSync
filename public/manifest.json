{
    "manifest_version": 3,
    "name": "TMSync",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["identity", "storage"],
    "host_permissions": ["https://api.trakt.tv/*/"],
    "content_scripts": [
        {
            "js": ["js/contentScript.js"],
            "matches": [
                "https://www.cineby.app/*",
                "https://freek.to/*",
                "https://popcornmovies.to/*",
                "https://mcloud.bz/*",
                "https://vid2faf.site/*",
                "https://vidsrc.pro/*",
                "https://embed.su/*",
                "https://*.autoembed.cc/*",
                "https://vidlink.pro/*",
                "https://*.videasy.net/*",
                "https://*.2embed.cc/*"
            ],
            "all_frames": true
        }
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "key": ""
}
